{
  "name": "api-auto",
  "version": "1.0.0",
  "description": "API REST para gesti√≥n de sistema de autos",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "prod": "NODE_ENV=production node dist/server.js",
    "prod:win": "set NODE_ENV=production&& node dist/server.js",
    "build:prod": "npm run build && npm run prod",
    "build:prod:win": "npm run build && npm run prod:win",
    "deploy": "bash scripts/deploy.sh",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop api-autos",
    "pm2:restart": "pm2 restart api-autos"
  },
  "keywords": [
    "api",
    "typescript",
    "express",
    "mysql",
    "sequelize"
  ],
  "author": "",
  "license": "ISC",
